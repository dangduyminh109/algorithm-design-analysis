# Environment variables for Vercel deployment
# Copy this file to .env.local for local development

# Next.js Configuration
NEXT_PUBLIC_APP_NAME="Algorithm Complexity Visualizer"
NEXT_PUBLIC_APP_DESCRIPTION="Interactive algorithm complexity visualization platform"
NEXT_PUBLIC_APP_URL="https://algorithm-complexity-visualizer.vercel.app"

# Development
NODE_ENV=production

# Analytics (optional - add your tracking IDs)
# NEXT_PUBLIC_GA_ID=
# NEXT_PUBLIC_HOTJAR_ID=

# Performance monitoring (optional)
# NEXT_PUBLIC_SENTRY_DSN=

# Feature flags
NEXT_PUBLIC_ENABLE_ANALYTICS=false
NEXT_PUBLIC_ENABLE_MONITORING=false
